

#include<stdio.h>
int main()
{
    int t,x,p=0,dig;
    scanf("%d",&t);
    for(int i=0; i<t;i++)
    {
        long long int n;
        scanf("%lld",&n);
        x=n;
        while(n!=0)
        {
            dig = n%10;
        if(dig!=0)
        {
            if(x%dig==0)
            p++;
        }
            n=n/10;
        } 
        
        printf("%d\n",p);
        
        p=0;
        
        
    }
    
}
