

#include <stdio.h>
int main()
{
    int t,n,k,i,j,count=0;
    scanf("%d",&t);
    for(i=0;i<t;i++)
    {
        count=0;
        scanf("%d %d",&n,&k);
        int arr[n];
        for(j=0;j<n;j++)
        {
            scanf("%d",&arr[j]);
        }
        for(j=0;j<n;j++)
        {
        if(arr[j]<=0)
         count++;
        }
        if(count<k)
        {
            printf("YES\n");
        }
        else 
        {
        printf("NO\n");
        }
    }
}
