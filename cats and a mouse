

#include <stdio.h>
int main()
{
    int n,x,y,z,i,a,b;
    scanf("%d",&n);
    
    for(i=0;i<n;i++)
    {
       scanf("%d %d %d",&x,&y,&z);
       a = z-x;
       b = z-y;
       if(a<0)
       {
           a = -1*a;
       } 
       if(b<0)
       {
           b= -1*b;
       }
       if(a==b)
       {
           printf("Mouse C\n");
       }
       else if(a<b)
       {
           printf("Cat A\n");
       }
       else 
       {
        printf("Cat B\n");   
       }
       
    }
}
