
#include <stdio.h>
int main(){
    
    long long int i,sum=0,min,max=0,arr[5];
    for(i=0;i<5;i++)
  {
    scanf("%lld",&arr[i]);
    
   if(arr[i]>max)
    {
      max = arr[i];    
    }
     sum += arr[i];
  }
     
min =arr[0];  
    for(i=0;i<5;i++)
   {
    if(arr[i]<min)
       {
    min = arr[i];    
       } 
   }
    printf("%lld %lld",sum-max,sum-min);
     
   }
  
    
    
    
    
